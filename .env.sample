# Mail Configuration
EMAIL_USER=YOUR_EMAIL_USERNAME
EMAIL_PASSWORD=YOUR_EMAIL_PASSWORD

# Frontend URL (for CORS configuration)
FRONTEND_URL=http://localhost:3000
